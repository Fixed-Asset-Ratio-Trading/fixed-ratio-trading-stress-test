{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "FixedRatioStressTest": "Debug"
    }
  },
  "GuiLogging": {
    "EnableFileLogging": true,
    "FileLogging": {
      "Path": "logs/gui.log",
      "MaxSizeKB": 10240,
      "MaxBackupFiles": 5
    },
    "Display": {
      "MaxEntries": 5000,
      "AutoScroll": true,
      "RefreshInterval": 100
    },
    "MinimumLevel": "Debug",
    "SuppressDebugMessages": false
  },
  "UdpListener": {
    "Enabled": true,
    "Port": 12345,
    "InjectIntoLocalLogging": false
  },
  "GuiSettings": {
    "WindowTitle": "Service Manager - Test Mode",
    "MaxLogEntries": 5000,
    "MaxDisplayEntries": 1000,
    "AutoScroll": true
  },
  "EnableConsoleLogging": false,
  "SolanaConfig": {
    "ClusterUrl": "http://localhost:8899",
    "CommitmentLevel": "processed",
    "RequestTimeoutMs": 30000,
    "MaxRetries": 3,
    "RetryDelayMs": 1000,
    "AccountLoadBatchSize": 100
  },
  "StorageConfig": {
    "DataDirectory": "data"
  }
}